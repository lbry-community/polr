{
    "name": "cydrobolt/polr",
    "description": "The Polr URL Shortener.",
    "keywords": ["url-shortener", "url", "cms"],
    "license": "GPLv2+",
    "type": "project",
    "require": {
        "php": ">=7.1",
        "laravel/lumen-framework": "6.3.0.*",
        "vlucas/phpdotenv": "^3.6",
        "illuminate/mail": "6.8.0",
        "yajra/laravel-datatables-oracle": "9.8.0",
        "paragonie/random_compat": "9.99.99",
        "torann/geoip": "1.0.13",
        "geoip2/geoip2": "2.10.0",
        "nesbot/carbon": "2.28.0",
        "doctrine/dbal": "2.10.1",
        "google/recaptcha": "1.2.3"
    },
    "require-dev": {
        "fzaninotto/faker": "1.9.1",
        "bamarni/composer-bin-plugin": "^1.3",
        "phpunit/phpunit": "^7",
        "symfony/css-selector": "5.0.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
